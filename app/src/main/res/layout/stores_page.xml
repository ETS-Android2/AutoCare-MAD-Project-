<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/papers_co_ad74_bmw_car_black_light_41_iphone_wallpaper">

    <TextView
        android:id="@+id/textView"
        android:layout_width="204dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="169dp"
        android:text="@string/Stores"
        android:textColor="@color/white"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_alignStart="@+id/textView"
        android:layout_marginStart="0dp"
        android:layout_marginTop="2dp"
        android:text="@string/StoresDS"
        android:textColor="@color/white"
        android:textSize="15sp" />

    <View
        android:id="@+id/view1"
        android:layout_width="365dp"
        android:layout_height="50dp"
        android:layout_below="@+id/textView4"
        android:layout_alignParentStart="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/table_style" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="72dp"
        android:layout_height="20dp"
        android:layout_alignStart="@+id/view1"
        android:layout_alignTop="@+id/view1"
        android:layout_alignBottom="@+id/view1"
        android:layout_marginStart="19dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="14dp"
        android:text="Item Code"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="94dp"
        android:layout_height="25dp"
        android:layout_alignTop="@+id/view1"
        android:layout_alignBottom="@+id/view1"
        android:layout_marginStart="3dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="14dp"
        android:layout_toEndOf="@+id/textView12"
        android:text="Item Quantity"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="81dp"
        android:layout_height="21dp"
        android:layout_alignTop="@+id/view1"
        android:layout_alignBottom="@+id/view1"
        android:layout_marginStart="3dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="14dp"
        android:layout_toEndOf="@+id/textView9"
        android:text="Item Name"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="81dp"
        android:layout_height="21dp"
        android:layout_alignTop="@+id/view1"
        android:layout_alignBottom="@+id/view1"
        android:layout_marginStart="3dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="14dp"
        android:layout_toEndOf="@+id/textView10"
        android:text="Item Name"
        android:textColor="@color/white" />

    <ListView
        android:id="@+id/getList"
        android:layout_width="365dp"
        android:layout_height="499dp"
        android:layout_below="@+id/view1"
        android:layout_alignStart="@+id/view1"
        android:layout_alignEnd="@+id/view1"
        android:layout_marginStart="0dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="0dp" />

</RelativeLayout>